{
  "name": "hackfest",
  "homepage": "https://github.com/mdirshaddev/hackfest#readme",
  "version": "1.0.0",
  "description": "A Modern full-fledged Admin and User Control based Blogging web application",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "prettier-write": "prettier --write .",
    "check-lint": "eslint .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "test": "echo \"Write tests!\" && exit 0",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "studio": "prisma studio",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:schema",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P tsconfig.nexus.json src/graphql/utilities/schema",
    "generate:schema": "graphql-codegen --config codegen.js",
    "vercel-build": "yarn generate && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdirshaddev/hackfest.git"
  },
  "bugs": {
    "url": "https://github.com/mdirshaddev/hackfest/issues"
  },
  "author": "Md Irshad <mdirshaddev+github@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "pwa"
  ],
  "engines": {
    "node": ">=16.15.0"
  },
